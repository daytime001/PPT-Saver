{
   "name": "__MSG_extensionName__",
   "version": "1.0",
   "manifest_version": 3,
   "description": "__MSG_extensionDescription__",
   "default_locale": "zh_CN",
   "action": {
      "default_title": "__MSG_actionTitle__",
      "default_icon": {
         "16": "icons/icon16.png",
         "32": "icons/icon32.png",
         "48": "icons/icon48.png",
         "128": "icons/icon128.png"
      }
   },
   "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   },
   "content_scripts": [
      {
         "matches": [
            "https://*.ketangpai.com/*"
         ],
         "run_at": "document_start",
         "js": [
            "inject.js"
         ]
      }
   ],
   "permissions": [
      "activeTab",
      "storage"
   ],
   "host_permissions": [
      "*://*.ketangpai.com/*"
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "js/*"
         ],
         "matches": [
            "https://*.ketangpai.com/*"
         ]
      }
   ]
}